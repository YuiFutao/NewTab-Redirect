{
  "manifest_version": 2,
  "name": "colors-of-the-day",
  "short_name": "CotD",
  "description": "曜日を連想させる写真を新しいタブに表示します",
  "version": "1.1",
  "chrome_url_overrides": {
    "newtab": "main.html"
  },
  "permissions": [],
  "optional_permissions": [
    "tabs"
  ],
  "icons": {
    "128": "icon/icon128.png",
    "48": "icon/icon48.png",
    "16": "icon/icon16.png"
  },
  "incognito": "split",
  "minimum_chrome_version": "22"
}
